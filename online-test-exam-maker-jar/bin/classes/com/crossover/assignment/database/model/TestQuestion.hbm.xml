<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.crossover.assignment.database.model">

	<class name="TestQuestion" table="test_questions">
		<composite-id>
			<key-many-to-one name="test"
				class="com.crossover.assignment.database.model.Test" column="test_id"></key-many-to-one>
			<key-many-to-one name="question"
				class="com.crossover.assignment.database.model.Question" column="question_id"></key-many-to-one>
		</composite-id>
	</class>

</hibernate-mapping>
